<% if (typeof links !== 'undefined' && links && links.length > 0) { %>
<div class="links">
  <%- include('../section-title/index.ejs', {title: '友情链接'}) %>
  <div class="list">
    <% links.forEach(function(link) { %>
      <a href="<%= link.url %>" target="_blank"><%= link.name %></a>
    <% }) %>
  </div>
</div>
<% } %>