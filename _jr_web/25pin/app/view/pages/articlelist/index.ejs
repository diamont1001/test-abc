<%- include('../../modules/header/index.ejs', {}) %>

<% if (typeof tagsList !== 'undefined' && tagsList.length > 0) { %>
  <section class="tags">
    <%- include('../../modules/tags/index.ejs', {path: '/article', list: tagsList, curTag: curTagId}) %>
  </section>
<% } %>

<section class="article">
  <ul class="list">
    <% if (typeof articleList !== 'undefined' && articleList.length > 0) { %>
      <%- include('./list.ejs', {articleList: articleList, dateFormat: dateFormat}) %>
    <% } %>
  </ul>
  <% if (typeof articleList !== 'undefined' && articleList.length > 0) { %>
    <%- include('../../modules/more/index.ejs', {url: 'javascript:;'}) %>
  <% } %>
</section>

<% if (typeof curTagId !== 'undefined' && curTagId) { %>
  <div id="dataTag" data-tag="<%= curTagId %>" style="display: none;"></div>
<% } %>

<% if (typeof listType !== 'undefined' && listType) { %>
<div id="dataListType" data-type="<%= listType %>" style="display: none;"></div>
<% } %>